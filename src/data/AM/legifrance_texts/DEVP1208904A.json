        },
        {
            "articleVersion": "1.0",
            "cid": "JORFARTI000025687414",
            "comporteLiens": false,
            "conditionDiffere": null,
            "content": "<p><br/>Le directeur général de la prévention des risques est chargé de l'exécution du présent arrêté, qui sera publié au Journal officiel de la République française.</p>",
            "dereferenced": false,
            "etat": "VIGUEUR",
            "executionTime": 0,
